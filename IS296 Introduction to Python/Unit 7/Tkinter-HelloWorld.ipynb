   "source": [
    "# this creates a window \n",
    "root = tk.Tk()\n",
    "\n",
    "# Text to print out hello world\n",
    "my_label = tk.Label(root, text=\"Hello World\")\n",
    "my_label.pack()\n",
    "\n",
    "# Keeps the widow in display mode\n",
    "root.mainloop()"
   ]
